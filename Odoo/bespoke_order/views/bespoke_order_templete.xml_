                                    <!--Bar button-->
                                    <div class="barButton">
                                        <a class="hvr-underline-from-right hvr-box-shadow-outset pull-left text-btn"
                                           onclick="toShowBack(jQuery(this));" role="button">Back
                                        </a>
                                        <a class="hvr-underline-from-left hvr-box-shadow-outset pull-right text-btn"
                                           onclick="validateScheduleSlide(jQuery(this));"
                                           data-factor-name="schedule"
                                           data-factor-value="schedule" data-selection-type="single"
                                           data-resume-name="Diet Schedule" role="button">Next
                                        </a>
                                    </div>
                                </div>

                                <!--Extras & Exclusions -->
                                <div class="slide row-fluid hide" id="step8">

                                    <div class="row">
                                        <h2 class="text-header">Pick up your extras</h2>
                                        <div class="row-fluid text-center containerExtras">


                                        </div>
                                        <h2 class="text-header">Select your exclusions</h2>
                                        <div class="container containerExclusions">
                                            <div id="pickList"></div>
                                        </div>
                                    </div>
                                    <div class="notificationChargesExclusions">

                                    </div>

                                    <!--Bar button-->
                                    <div class="barButton">
                                        <a class="hvr-underline-from-right hvr-box-shadow-outset pull-left text-btn"
                                           onclick="toShowBack(jQuery(this));" role="button">Back
                                        </a>
                                        <a class="hvr-underline-from-left hvr-box-shadow-outset pull-right text-btn"
                                           onclick="toShowNext(jQuery(this));addBtnContinueToResume(jQuery(this));"
                                           data-factor-name="extra_exclusion" data-selection-type="single"
                                           data-factor-value="extra_exclusion"
                                           data-resume-name="Extras and Exclusions"
                                           role="button">Next
                                        </a>
                                    </div>
                                </div>

                                <!--Extras & Exclusions -->
                                <div class="slide row-fluid hide" id="step9">
                                    <div class="row">

                                        <div class="loader-container" id="summaryLoader">
                                            <div class="loader">Loading...</div>
                                            <h4 style="margin-top: 10px;">Preparing your order...</h4>
                                        </div>
                                        <div class="col-md-12 summaryOrder ">
                                            <div class="panel panel-default">
                                                <div class="panel-heading">
                                                    <h5 class="text-center">
                                                        <strong>Order summary</strong>
                                                    </h5>
                                                </div>
                                                <div class="panel-body">
                                                    <div class="table-responsive">
                                                        <table class="table table-condensed table-hover">
                                                            <caption class="invoice-caption">Selections</caption>
                                                            <thead>
                                                                <tr>
                                                                    <td>
                                                                        <strong>Your Goal</strong>
                                                                    </td>
                                                                    <td class="text-center">
                                                                        <strong>Meal Type</strong>
                                                                    </td>
                                                                    <td class="text-center">
                                                                        <strong>Juice Cleanse</strong>
                                                                    </td>
                                                                    <td class="text-right">
                                                                        <strong>Post Code</strong>
                                                                    </td>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr>
                                                                    <td id="goal_plan">example plan</td>
                                                                    <td class="text-center" id="meal_type">Meal Type
                                                                    </td>
                                                                    <td class="text-center" id="juice_cleanse">Juice
                                                                        Cleanse
                                                                    </td>
                                                                    <td class="text-right" id="post_code">Post
                                                                        Code
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>

                                                    <div class="table-responsive">
                                                        <table class="table table-condensed your-macros table-hover">
                                                            <caption class="invoice-caption">Your Macros</caption>
                                                            <thead>
