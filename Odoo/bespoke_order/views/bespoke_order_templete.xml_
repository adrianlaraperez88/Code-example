<openerp>
    <data>


        <template id="order_templete">
            <t t-call="website.layout">
                <div class="oe_structure">
                    <div class="container wrap-shadow">
                        <script type="text/javascript">
                            odoo.define('web.csrf', function (require) {
                            var token = "<t t-esc="request.csrf_token(None)"/>";
                            require('web.core').csrf_token = token;
                            require('qweb').default_dict.csrf_token = token;
                            });
                        </script>
                        <div class="row-fluid">
                            <div class="col-xs-12 col-sm-12 col-md-8 col-lg-8 ">

                                <input type="hidden" id="csrf_token" name="csrf_token"
                                       t-att-value="request.csrf_token(None)"/>

                                <!--Goal Plans-->
                                <div class="slide row-fluid " id="step1">
                                    <div class="row">
                                        <h2 class="text-header">Select your goal</h2>
                                    </div>
                                    <t t-foreach="type_plan" t-as="plan">

                                        <div class="row-fluid ">
                                            <a class="buttonPlan large" data-selection-type="single"
                                               onclick="btnClick(jQuery(this));" data-factor-name="goal_plan"
                                               t-att="{'data-factor-value': plan.id,'data-invoice-title':plan.name}"
                                               href="#"
                                               role="button">
                                                <p class="text-btn">
                                                    <t t-esc="plan.name"/>
                                                </p>
                                            </a>
                                        </div>
                                    </t>
                                </div>

                                <!--Meal Types-->
                                <div class="slide row-fluid hide" id="step2">
                                    <div class="row">
                                        <h2 class="text-header">Select your Meal Type</h2>
                                    </div>

                                    <t t-foreach="category" t-as="cat">

                                        <div t-if="category[len(category)-1] == cat and len(category)%2 != 0"
                                             class="col-xs-12 ">
                                            <a class="buttonPlan col-xs-12 col-md-6"
                                               t-att="{'data-factor-value': cat.name,'data-invoice-title':cat.name}"
                                               data-selection-type="single"
                                               onclick="btnClick(jQuery(this));" data-factor-name="meal_type"
                                               href="#" role="button">
                                                <p class="text-btn">
                                                    <t t-esc="cat.name"/>
                                                </p>
                                            </a>
                                        </div>

                                        <div t-if="category[len(category)-1] != cat"
                                             class="col-xs-12 col-md-6 ">
                                            <a class="buttonPlan col-xs-12 col-md-6"
                                               t-att="{'data-factor-value': cat.name,'data-invoice-title':cat.name}"
                                               data-selection-type="single"
                                               onclick="btnClick(jQuery(this));" data-factor-name="meal_type"
                                               href="#" role="button">
                                                <p class="text-btn">
                                                    <t t-esc="cat.name"/>
                                                </p>
                                            </a>
                                        </div>
                                        <div t-if="len(category) == 0" class="col-xs-12">
                                            <a class="buttonPlan col-xs-12 col-md-6" href="#" role="button">
                                                <p class="text-btn">Sorry, not available now.</p>
                                            </a>
                                        </div>
                                    </t>
                                    <div class="barButton">
                                        <a class="hvr-underline-from-right hvr-box-shadow-outset pull-left text-btn"
                                           onclick="toShowBack(jQuery(this));" role="button">Back
                                        </a>
                                        <div class="pull-right hide" id="loader">
                                            <div class="loader pull-left">Loading...</div>
                                            <b style="margin-top: 10px;" class="pull-right">Calculating your
                                                extras and exclusions...
                                            </b>
                                        </div>
                                    </div>
                                </div>

                                <!--Juice Cleanse-->
                                <div class="slide row-fluid hide" id="step3">
                                    <div class="row">
                                        <h2 class="text-header">Juice Cleanse</h2>
                                    </div>

                                    <div class="col-xs-12 col-md-6 ">
                                        <a class="buttonPlan " data-selection-type="single"
                                           onclick="btnClick(jQuery(this));" data-factor-name="juice_cleanse"
                                           data-factor-value="true"
                                           data-invoice-title="Start my plan with a Juice Clenase" href="#"
                                           role="button">
                                            <p class="text-btn">Start my plan with a Juice Clenase</p>
                                        </a>
                                    </div>
                                    <div class="col-xs-12 col-md-6">
                                        <a class="buttonPlan " data-selection-type="single"
                                           onclick="btnClick(jQuery(this));" data-factor-name="juice_cleanse"
                                           data-factor-value="false" data-invoice-title="No Juice Cleanse for now"
                                           href="#" role="button">
                                            <p class="text-btn">No Juice Cleanse for now</p>
                                        </a>
                                    </div>

                                    <!--Bar button-->
                                    <div class="barButton">
                                        <a class="hvr-underline-from-right hvr-box-shadow-outset pull-left text-btn"
                                           onclick="toShowBack(jQuery(this));" role="button">Back
                                        </a>
                                    </div>
                                </div>

                                <!--Post Code Verification-->
                                <div class="slide row-fluid hide" id="step4">
                                    <div class="row">
                                        <h2 class="text-header">Check your post code</h2>
                                    </div>

                                    <div class="col-xs-12 col-md-6 ">
                                        <input type="text" name="postcode" class="postcode"
                                               placeholder="Post Code"/>
                                    </div>
                                    <div class="col-xs-12 col-md-6">
                                        <a class="buttonPlan " data-selection-type="single"
                                           data-factor-name="post_code"
                                           data-factor-value="" data-invoice-title=""
                                           onclick="btnClick(jQuery(this));"
                                           href="#" role="button">
                                            <p class="text-btn">
                                                <i class="fa fa-check"></i>
                                                Check Now
                                            </p>
                                        </a>
                                    </div>

                                    <!--Bar button-->
                                    <div class="barButton">
                                        <a class="hvr-underline-from-right hvr-box-shadow-outset pull-left text-btn"
                                           onclick="toShowBack(jQuery(this));" role="button">Back
                                        </a>
                                    </div>
                                </div>

                                <!--Sex-->
                                <div class="slide row-fluid hide" id="step5">
                                    <div class="row ">
                                        <h2 class="text-header">Please, provide your sex</h2>
                                    </div>
                                    <div class="row-fluid ">
                                        <a class="buttonAction" data-selection-type="single"
                                           onclick="btnClick(jQuery(this));" data-factor-name="sex"
                                           data-factor-value="male" href="#" role="button">
                                            <p class="text-btn">Maled</p>
                                        </a>
                                        <a class="buttonAction" data-selection-type="single"
                                           onclick="btnClick(jQuery(this));" data-factor-name="sex"
                                           data-factor-value="female" href="#" role="button">
                                            <p class="text-btn">Femaleo</p>
                                        </a>
                                    </div>
                                    <!--Bar button-->
                                    <div class="barButton">
                                        <a class="hvr-underline-from-right hvr-box-shadow-outset pull-left text-btn"
                                           onclick="toShowBack(jQuery(this));" role="button">Back
                                        </a>
                                    </div>
                                </div>

                                <!--About You -->
                                <div class="slide row-fluid hide" id="step6">
                                    <div class="row bordered stats">
                                        <h2 class="text-header">About you</h2>

                                        <div class="col-md-4">
                                            <label for="weight">Weight (Kg)</label>
                                            <input type="text" class="form-control form-control-extended number"
                                                   data-rule-required="true" id="weight"
                                                   placeholder="Weight (Kilos)"/>
                                            <div class="row">
                                                <div class="col-xs-7 col-sm-7">
                                                    <label for="weight">Stones</label>
                                                    <input id="stones" type="text"
                                                           class="form-control form-control-extended stones"
                                                           placeholder="Stones"/>
                                                </div>
                                                <div class="col-xs-5 col-sm-5">
                                                    <label for="weight">Pounds</label>
                                                    <input id="pounds" type="text"
                                                           class="form-control form-control-extended "
                                                           placeholder="Pounds"/>
                                                </div>
                                            </div>


                                        </div>
                                        <div class="col-md-4">
                                            <label for="height">Height (Cm)</label>
                                            <input type="text" class="form-control form-control-extended number"
                                                   data-rule-required="true" id="height"
                                                   placeholder="Height (Centimeters)"/>
                                            <div class="row">
                                                <div class="col-xs-7 col-sm-7">
                                                    <label for="weight">Feet</label>
                                                    <input id="feets" type="text"
                                                           class="form-control form-control-extended "
                                                           placeholder="Feet"/>
                                                </div>
                                                <div class="col-xs-5 col-sm-5">
                                                    <label for="weight">Inches</label>
                                                    <input id="inches" type="text"
                                                           class="form-control form-control-extended "
                                                           placeholder="Inches"/>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label class="pull-left" for="db">Date of Birth (DD/MM/YYYY)</label>
                                                <input type="text" t-att="{'readonly' : readonly}"
                                                       data-rule-required="true"
                                                       class="form-control form-control-extended datepicker" id="db"
                                                       placeholder="Date of Birth (DD/MM/YYYY)"/>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row bordered activity">
                                        <h2 class="text-header">Activity</h2>
                                        <div class="col-md-4">
                                            <div id="al" class="form-group ">
                                                <label class="pull-left" for="al">ACTIVITY LEVEL DURING THE DAY
                                                </label>
                                                <select class="form-control form-control-extended " id="actLevel"
                                                        data-rule-required="true">
                                                    <option value="-1">Select</option>
                                                    <option value="1.2">Sedentary</option>
                                                    <option value="1.375">Lightly Active</option>
                                                    <option value="1.55">Moderately Active</option>
                                                    <option value="1.725">Very Active</option>
                                                    <option value="1.9">Extremely active</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label class="pull-left" for="bfp">BODY FAT PERCENTAGE</label>
                                                <input type="text" class="form-control form-control-extended number"
                                                       id="bfp" placeholder="Body fat Percentage (%)"/>
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label class="pull-left" for="bp">DO YOU KNOW YOUR MACROS ?</label>
                                                <label class="checkbox-inline"><input type="checkbox" id="ikmacros"
                                                                                      value="ikmacros"/>Yes, i know
                                                </label>
                                            </div>
                                        </div>

                                    </div>
                                    <div class="row bordered macros">
                                        <h2 class="text-header">Specify your Daily Macros</h2>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label class="pull-left">Protein (grams)</label>
                                                <input type="text" id="protein" data-rule-required="true"
                                                       class="form-control form-control-extended number protein"
                                                       placeholder="Protein (grams)"/>
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label class="pull-left">Fat (grams)</label>
                                                <input type="text" id="fat" data-rule-required="true"
                                                       class="form-control form-control-extended number fat"
                                                       placeholder="Fat (grams)"/>
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label class="pull-left">Carbs (grams)</label>
                                                <input type="text" id="carbs" data-rule-required="true"
                                                       class="form-control form-control-extended number carbs"
                                                       placeholder="Carbs (grams)"/>
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label class="pull-left"></label>
                                                <input type="text" id="totalMacros" t-att="{'disabled': disabled}"
                                                       class="form-control  form-control-extended total"
                                                       data-rule-required="true" placeholder="Total"/>
                                            </div>
                                        </div>

                                    </div>

                                    <!--Bar button-->
                                    <div class="barButton">
                                        <a class="hvr-underline-from-right hvr-box-shadow-outset pull-left text-btn"
                                           onclick="toShowBack(jQuery(this));" role="button">Back
                                        </a>
                                        <a class="hvr-underline-from-left hvr-box-shadow-outset pull-right text-btn"
                                           onclick="validateMacroSlide(jQuery(this));" data-factor-name="macros"
                                           data-factor-value="macros" data-selection-type="single"
                                           data-resume-name="Diet Requirements" role="button">Next
                                        </a>
                                    </div>
                                </div>

                                <!--How many meals per day or week-->
                                <div class="slide row-fluid hide" id="step7">
                                    <div class="row text-center mpd">
                                        <h3 class="text-header">How many meals per day</h3>

                                        <div class="col-xs-12 col-sm-6 col-md-4">
                                            <a class="buttonPlan large" data-selection-type="single"
                                               onclick="btnScheduleClick(jQuery(this));"
                                               data-factor-name="meals_day"
                                               data-factor-value="3" href="#" role="button">
                                                <p class="text-btn">3 Meals</p>
                                            </a>
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-4">
                                            <a class="buttonPlan large" data-selection-type="single"
                                               onclick="btnScheduleClick(jQuery(this));"
                                               data-factor-name="meals_day"
                                               data-factor-value="4" href="#" role="button">
                                                <p class="text-btn">4 Meals</p>
                                            </a>
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-4">
                                            <a class="buttonPlan large" data-selection-type="single"
                                               onclick="btnScheduleClick(jQuery(this));"
                                               data-factor-name="meals_day"
                                               data-factor-value="5" href="#" role="button">
                                                <p class="text-btn">5 Meals</p>
                                            </a>
                                        </div>

                                    </div>
                                    <br/>
