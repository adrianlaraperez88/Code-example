                                        <div class="toshowSlide" id="exclusions" data-slide="step3">
                                            <p class="label">Exclusions/Allergies</p>
                                            <p class="value">..</p>
                                        </div>
                                        <div class="toshowSlide" id="numberofmeals" data-slide="step3">
                                            <p class="label">Meal Number Per Day</p>
                                            <p class="value">..</p>
                                        </div>

                                    </div>
                                </div>
                            </div>
                            <br/>
                            <p>
                                <a href="mailto:hello@bespokefitnessfuel.com">hello@bespokefitnessfuel.com</a>
                            </p>
                            <div id='next_btn' class="col-lg-12">
                                <a class="hvr-underline-from-right hvr-box-shadow-outset pull-left text-btn"
                                   id="toShowNext" role="button">Next
                                </a>
                            </div>
                        </div>


                    </div>

                </div>
                <t t-call="bespoke_order.wizard_order"/>
            </t>
        </template>

        <template id="ecommerce_confirmation_bespoke" inherit_id="website_sale.confirmation">
            <xpath expr="//div[@id='wrap']//div[@class='row']//div[@class='oe_cart']/table[1]/tbody/tr[2]"
                   position="replace">

                <tr>
                    <td></td>
                    <td></td>
                    <td></td>

                    <td>
                        <strong>Exclusions:</strong>

                    </td>
                    <td class="text-right">
                        <strong t-field="order.pay_exclusions"
                                t-field-options='{"widget": "monetary", "display_currency": "order.pricelist_id.currency_id"}'/>

                    </td>
                </tr>
                <tr>
                    <td></td>
                    <td></td>
                    <td></td>

                    <td class="text-right">
                        <strong>-Discount:</strong>
                    </td>
                    <td class="text-right">
                        <strong t-field="order.discount_amt"
                                t-field-options='{"widget": "monetary", "display_currency": "order.pricelist_id.currency_id"}'/>
                    </td>
                </tr>
                <tr>
                    <td></td>
                    <td></td>
                    <td></td>

                    <td>
                        <strong>Total:</strong>
                    </td>
                    <td class="text-right">
                        <strong t-field="order.amount_total"
                                t-field-options='{"widget": "monetary", "display_currency": "order.pricelist_id.currency_id"}'/>
                    </td>
                </tr>
            </xpath>

        </template>

        <template id="ecommerce_total_bespoke_pay_exclusions" inherit_id="website_sale.total">
            <xpath expr="//div[@id='cart_total']/div[@id='order_total_taxes']" position="after">


                <div class="row" id="pay_contract_bespoke">
                    <span class="col-xs-6 text-right text-muted"
                          title="Discount for type contract">Contract:
                    </span>
                    <span class="col-xs-6 text-left text-muted">
                        <span t-field="website_sale_order.pay_contract_bespoke" style="white-space: nowrap;"
                              t-field-options='{
                          "widget": "monetary",
                          "from_currency": "website_sale_order.pricelist_id.currency_id",
                          "display_currency": "website.currency_id"
                      }'/>
                    </span>
                </div>

                <div class="row" id="order_pay_exclusions">
                    <span class="col-xs-6 text-right text-muted"
                          title="Pay for exclusions may be updated after providing shipping address">Exclusions:
                    </span>
                    <span class="col-xs-6 text-left text-muted">
                        <span t-field="website_sale_order.pay_exclusions" style="white-space: nowrap;" t-field-options='{
                          "widget": "monetary",
                          "from_currency": "website_sale_order.pricelist_id.currency_id",
                          "display_currency": "website.currency_id"
                      }'/>
                    </span>
                </div>

                <div class="row" id="order_discount_contract">
                    <span class="col-xs-6 text-right text-muted"
                          title="Discount for type contract">-Discount:
                    </span>
                    <span class="col-xs-6 text-left text-muted">
                        <span t-field="website_sale_order.discount_amt" style="white-space: nowrap;" t-field-options='{
                          "widget": "monetary",
                          "from_currency": "website_sale_order.pricelist_id.currency_id",
                          "display_currency": "website.currency_id"
                      }'/>
                    </span>
                </div>

            </xpath>
        </template>

        <template id="info_contract">
            <div class="row">
                <div class="col-md-8 col-sm-9 oe_cart">
                    <table class="table table-striped table-condensed js_cart_lines" id="cart_products"
                           t-if="bki and bki.sale_order_id">
                        <thead>
                            <tr>
                                <th width="100" class="text-center">Contract</th>
                                <th width="130" class="text-center">Discount</th>
                                <th width="100" class="text-center">Meals per Day</th>
                                <th width="100" class="text-center">Price per day</th>
                                <th width="100" class="text-center">Total</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td align="center">
                                    <span t-field="bki.bc_id"/>
                                </td>
                                <td align="center">
                                    <span t-field="bki.discount_amount" t-field-options='{
                    "widget": "monetary",
                    "from_currency": "bki.sale_order_id.currency_id",
                    "display_currency": "website.currency_id"
                    }'/>
                                </td>

                                <td align="center">
                                    <span t-field="bki.meal_number_id"/>
                                </td>


                                <td align="center">
                                    <span t-field="bki.price_meal_day" t-field-options='{
                    "widget": "monetary",
                    "from_currency": "bki.sale_order_id.currency_id",
                    "display_currency": "website.currency_id"
                    }'/>
                                </td>


                                <td align="center">
                                    <span t-field="bki.pay_per_contract" t-field-options='{
                    "widget": "monetary",
                    "from_currency": "bki.sale_order_id.currency_id",
                    "display_currency": "website.currency_id"
                    }'/>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </template>

        <template id="ecommerce_cart_bespoke" inherit_id="website_sale.cart">


            <xpath expr="//div[@id='wrap']/div[@class='container oe_website_sale']/div[@class='row']/div[@id='right_column']"
                   position="inside">
                <a t-if="website_sale_order and website_sale_order.website_order_line"
                   class="btn btn-primary pull-banner mb32" href="/order">
                    <span class="hidden-xs">Edit Order</span>
                </a>
            </xpath>

            <xpath expr="//div[@id='wrap']/div[@class='container oe_website_sale']/div[@class='row']" position="before">
                <t t-call="bespoke_order.info_contract"/>
            </xpath>
        </template>


